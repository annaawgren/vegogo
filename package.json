{
  "scripts": {
    "precommit": "lint-staged",
    "start": "concurrently \"npm run start-keystone\" \"npm run start-frontend\"",
    "start-keystone": "cd keystone && node keystone.js",
    "start-frontend": "cd frontend && npm start"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "concurrently": "^3.5.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "1.13.4"
  }
}
