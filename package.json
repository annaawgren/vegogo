{
  "scripts": {
    "precommit": "lint-staged",
    "start": "concurrently \"npm run start-keystone\" \"npm run start-frontend\"",
    "start-keystone": "cd keystone && npm run watch",
    "start-frontend": "cd frontend && npm start"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "concurrently": "^3.6.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.2"
  }
}
